@{
    ViewData["Title"] = "Home Page";
}


<form asp-action="Index" asp-controller="Home" method="post" enctype="multipart/form-data">
    <input type="file" name="file" /><br>
    <input type="submit" value="Загрузить" />
</form>

@if (Model != null)
{
    <table>
        <tr>
            <td>Name</td>
            <td>Score</td>
        </tr>
        @foreach (var label in Model)
        {
            <tr>
                <td>@label.Label</td>
                <td>@label.Score</td>
            </tr>
        }
    </table>
}